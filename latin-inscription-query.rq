SELECT DISTINCT ?item ?itemLabel ?image ?inscription ?translation ?contains ?position
WHERE 
{
  ?item wdt:P8583 "34184";
        wdt:P18 ?image;
        p:P1684 ?statement1.
  ?statement1 ps:P1684 ?inscription.
  ?statement1 pq:P2441 ?translation.
  ?item p:P180 ?statement.
  ?statement ps:P180 ?contains;
             pq:P2677 ?position.
 
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". }
}